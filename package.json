{
  "name": "trabajoya-backend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "vercel-build": "prisma generate && nest build && (npm run prisma:deploy && npm run prisma:seed-if-empty || echo '⚠️ Migraciones o seed fallaron, pero el build continúa')",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts",
    "prisma:seed-if-empty": "ts-node --transpile-only prisma/seed-if-empty.ts",
    "vercel:postdeploy": "npm run prisma:deploy && npm run prisma:seed-if-empty",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:down": "docker-compose down",
    "docker:down:prod": "docker-compose -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.916.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.0",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^5.22.0",
    "@types/jsonwebtoken": "^9.0.10",
    "apple-signin-auth": "^2.0.0",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "country-state-city": "^3.2.1",
    "google-auth-library": "^10.4.2",
    "helmet": "^7.1.0",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.4",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "prisma": "^5.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
